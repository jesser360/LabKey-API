<!DOCTYPE html>
<html>
  <head>
    <title>ANimal Housing Report</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
<link href="https://fonts.googleapis.com/css?family=Amatic+SC" rel="stylesheet">
  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

  <!-- Latest compiled and minified JavaScript -->
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-confirmation/1.0.5/bootstrap-confirmation.js"></script>
  </head>
  <body>
  <div class='container'>
    <h3><%=link_to 'Back to Search', '/'%></h3>
    <h4>Searched Id: <%= @animal.participantId%></h4>

    <table style="width:100%">
      <tr>
        <th>Date In</th>
        <th>Date Out</th>
        <th>Cage</th>
        <th>Room</th>
        <th>Relocation #</th>
      </tr>
    <%10.times do |i|%>
      <%if @history_hash['rows'][i]%>
          <tr>
            <td><%= @history_hash['rows'][i]['date']%></td>
            <td><%= @history_hash['rows'][i]['enddate']%></td>
            <td><%= @history_hash['rows'][i]['cage']%></td>
            <td><%= @history_hash['rows'][i]['room']%></td>
            <td><%= @history_hash['rows'][i]['reloc_seq']%></td>
          </tr>
        <%end%>
    <%end%>
    </table>
  </div>
</body>
</html>

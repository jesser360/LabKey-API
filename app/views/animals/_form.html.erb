<%= form_with(model: animal, local: true) do |form| %>
  <% if animal.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(animal.errors.count, "error") %> prohibited this animal from being saved:</h2>

      <ul>
      <% animal.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<br>
  <div class="field">
    <%= form.label :Animal_ID %>
    <%= form.number_field :participantId, id: :animal_participantId, value:params['animal_id'] %>
  </div>

  <div class="field">
    <%= form.label :date_in %>
    <%= form.date_field :date, id: :animal_date %>
  </div>
<br>
  <!-- <div class="field">
    <%= form.label :date_out %>
    <%= form.date_field :enddate, id: :animal_end_date %>
  </div> -->
<br>
  <div class="field">
    <%= form.label :room %>
    <%= form.text_field :room, id: :room %>
<br>
 </div>
  <div class="field">
    <%= form.label :cage %>
    <%= form.text_field :cage, id: :animal_cage %>
  </div>


  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>


<span hidden id='room-array'>
  <%=@rooms%>
</span>
<script>
$( document).ready(function(){
  var tags = $('#room-array').text()
  var tags_arr = JSON.parse("[" + tags + "]")
  tags[0].replace('/[','/')
  $( "#room" ).autocomplete({
    source: tags_arr[0]
  });


});
</script>
